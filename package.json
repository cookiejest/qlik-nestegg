{
  "name": "Nestegg",
  "version": "0.1.2",
  "description": "Nestegg by w0w.io",
  "main": "main.js",
  "scripts": {
    "start": "./node_modules/.bin/electron .",
    "test": "mocha",
    "win": "electron-builder -w",
    "make-cert": "electron-builder create-self-signed-cert -p w0w.io"
  },
  "build": {
    "win": {
      "certificateFile": "private/w0w.io.pfx",
      "verifyUpdateCodeSignature": false,
      "publisherName": "w0w.io",
      "artifactName": "${name}-Setup-${version}.${ext}",
      "publish": [
        {
          "provider": "s3",
          "bucket": "nestegg-app"
        },
        {
          "provider": "github"
        }
      ]
    }
  },
  "author": "Adam Cooke (w0w.io)",
  "homepage": "nestegg.w0w.io",
  "license": "CC0-1.0",
  "devDependencies": {
    "devtron": "1.4.0",
    "electron": "1.7.8",
    "electron-reload": "1.2.2",
    "expect.js": "0.3.1",
    "mocha": "5.0.0"
  },
  "dependencies": {
    "bcrypt": "1.0.3",
    "electron-log": "2.2.14",
    "electron-titlebar": "0.0.3",
    "electron-updater": "2.20.1",
    "electron-window-state": "4.1.1",
    "enigma.js": "2.2.0",
    "expect.js": "0.3.1",
    "express": "4.16.2",
    "jquery": "3.2.1",
    "mocha": "5.0.0",
    "moment": "2.20.1",
    "request": "2.83.0",
    "valid-filename": "^2.0.1",
    "ws": "4.0.0"
  }
}
